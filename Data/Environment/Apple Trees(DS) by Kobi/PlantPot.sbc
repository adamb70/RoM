	<Component BuilderType="MyObjectBuilder_StockpileComponent" SubtypeId="StockpilePlanterPot" ForceCreate="true" /> -->
    <Component Type="Inventory" Subtype="InputInventory" />
    <Component Type="Inventory" Subtype="OutputInventory" />
    <Component Type="Inventory" Subtype="CraftingInventory"/>
    <Component Type="Inventory" Subtype="ProductInventory"/>
    <Component Type="InventorySpawnComponent"/>
    <Component Type="CraftingComponent" Subtype="PlanterPotCrafting"/>
    <Component Type="CraftingScheduler" Subtype="PlanterPotScheduler" />
    <Component Type="ConstantRecipeProviderComponent" Subtype="PlanterPotRecipes" />
