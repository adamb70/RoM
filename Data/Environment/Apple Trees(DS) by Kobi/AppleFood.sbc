<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <Definition xsi:type="MyObjectBuilder_CraftingRecipeDefinition">
    <Id Type="MyObjectBuilder_CraftingRecipeDefinition" Subtype="AppleTreeSapling"/>
    <DisplayName>Grow Apple tree Sapling</DisplayName>
    <Description>Rumour has it clay contains ancient apple tree seeds. Put it in the pot and wait.
	This could take an hour.</Description>
    <Icon>Textures\GUI\Icons\AppleTreeSapling.dds</Icon>
    <Category>SaplingGrowth</Category>
    <Prerequisites>
      <Item Amount="50" Type="InventoryItem" Subtype="Clay" />
    </Prerequisites>
    <Results>
      <Item Amount="1" Type="SeedBagHandItem" Subtype="AppleTreeSapling" />
    </Results>
    <CraftingTime Seconds="3600"/>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_SeedBagHandItemDefinition">
    <Id Type="SeedBagHandItem" Subtype="AppleTreeSapling"/>

    <DisplayName>Apple Tree Sapling</DisplayName>
    <Description>Place this sapling in the ground.
	It will need space to grow. Leave atleast 2m of space at each side</Description>
    <Icon>Textures\GUI\Icons\AppleTreeSapling.dds</Icon>
    <Size>
      <X>1.2</X>
      <Y>0.6</Y>
      <Z>0.3</Z>
    </Size>
    <Mass>0.5</Mass>
    <Model>Models\AppleTreeSapling.mwm</Model>

    <PhysicalMaterial>SeedBag</PhysicalMaterial>
    <MaxStackAmount>1</MaxStackAmount>
    <BearerDefinition Type="MyObjectBuilder_EntityEquipmentComponent" Subtype="Humanoid"/>
    <MaxDurability>1</MaxDurability>

    <Position>
      <Slot>MainHand</Slot>
    </Position>
    <Position>
      <Slot>OffHand</Slot>
    </Position>

    <StanceToBehavior>
      <StanceName>NormalMode</StanceName>
      <BehaviorId Type="MyObjectBuilder_SeedingToolBehaviorDefinition" Subtype="SeedingNormal"/>
    </StanceToBehavior>

    <GrowableDefinitionId>
      <TypeId>MyObjectBuilder_FarmableEnvironmentItemDefinition</TypeId>
      <SubtypeId>AppleTree</SubtypeId>
    </GrowableDefinitionId>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_ConsumableItemDefinition">
    <Id Type="ConsumableItem" Subtype="Apple" />
    <Public>true</Public>
    <DisplayName>Apple</DisplayName>
    <Description>a round red Fruit found on trees</Description>
    <Icon>Textures\GUI\Icons\Apple.dds</Icon>
    <Size>
      <X>0.1</X>
      <Y>0.1</Y>
      <Z>0.1</Z>
    </Size>
    <Mass>0.25</Mass>
    <Model>Models\PickedApple.mwm</Model>
    <PhysicalMaterial>None</PhysicalMaterial>
    <MaxStackAmount>20</MaxStackAmount>
    <Stats>
      <Stat Name="Food" Value="2" Clamped="true" />
    </Stats>
    <UseSound>PlayEat</UseSound>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_ConsumableItemDefinition">
    <Id Type="ConsumableItem" Subtype="RottenApple" />
    <Public>true</Public>
    <DisplayName>Rotten Apple</DisplayName>
    <Description>Do Not Eat</Description>
    <Icon>Textures\GUI\Icons\RottenApple.dds</Icon>
    <Size>
      <X>0.1</X>
      <Y>0.1</Y>
      <Z>0.1</Z>
    </Size>
    <Mass>0.25</Mass>
    <Model>Models\PickedRottenApple.mwm</Model>
