  </Definition>

  <Definition xsi:type="MyObjectBuilder_CraftingRecipeDefinition">
    <Id Type="MyObjectBuilder_CraftingRecipeDefinition" Subtype="ApplePie"/>
    <DisplayName>Bake an Apple Pie</DisplayName>
    <Description>Apples, flour and some herbs</Description>
    <Icon>Textures\GUI\Icons\ApplePie.dds</Icon>
    <Category>Meals</Category>
    <Prerequisites>
      <Item Amount="5" Type="ConsumableItem" Subtype="Apple" />
      <Item Amount="2" Type="ConsumableItem" Subtype="Herbs" />
      <Item Amount="1" Type="InventoryItem" Subtype="FlourWheat" />
    </Prerequisites>
    <Results>
      <Item Amount="2" Type="ConsumableItem" Subtype="ApplePie" />
    </Results>
    <CraftingTime Seconds="10"/>
  </Definition>

