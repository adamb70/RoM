  </Definition>

  <Definition xsi:type="MyObjectBuilder_CombatComponentDefinition">
    <Id Type="CombatComponent" Subtype="Humanoid"/>

    <SwingOrigin>
      <X>0</X>
      <Y>1.3</Y>
      <Z>0</Z>
    </SwingOrigin>

    <MiddleZoneAngle>60</MiddleZoneAngle>
    <SideZonesAngle>70</SideZonesAngle>
    <TopZoneHeight>1</TopZoneHeight>

    <BottomZoneHeight>.9</BottomZoneHeight>
    <CollisionRadius>.35</CollisionRadius>
  </Definition>

