  <Definition xsi:type="MyObjectBuilder_QuickEquipComponentDefinition">
    <Id Type="QuickEquipComponent" Subtype="Humanoid"/>
    <Item Action="CharacterTorch">
      <Id Type="EquipmentItem" Subtype="HandTorchItem"/>
    </Item>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_InventoryComponentDefinition">
    <Id Type="Inventory" Subtype="Internal"/>
    <InventoryName>Internal</InventoryName>
	  <DisplayName>DisplayName_InventoryName_Internal</DisplayName>
    <MaxItemCount>28</MaxItemCount>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_MultiComponentDefinition">
    <Id Type="AreaInventory" Subtype="Ground"/>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_MultiComponentDefinition">
    <Id Type="AreaInventoryAggregate" Subtype="NearbyInventories"/>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_CraftingComponentDefinition">
    <Id Type="CraftingComponent" Subtype="Humanoid" />
    <CraftingCategories>
      <Category>Tools</Category>
      <Category>Farming</Category>
      <Category>SmallBlocks</Category>
    </CraftingCategories>
    <ProductionQueueSize>4</ProductionQueueSize>
    <CraftingSpeedMultiplier>1</CraftingSpeedMultiplier>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_EntityQuestComponentDefinition">
    <Id Type="EntityQuestComponent" Subtype="Humanoid" />
    <InitialQuestId>RoMQuest_Tutorial_01</InitialQuestId>
  </Definition>

  <!-- Character stances -->
  <Definition xsi:type="MyObjectBuilder_EntityStateComponentDefinition">
    <Id Type="MyObjectBuilder_EntityStateComponent" Subtype="CharacterStances" />
    <InitialState>NormalMode</InitialState>
    <States>
      <State Name="NormalMode">
        <Transition>CombatMode</Transition>
      </State>
      <State Name="CombatMode">
        <Transition>NormalMode</Transition>
      </State>
    </States>
  </Definition>
  <!-- END OF "Character stances" -->

  <Definition xsi:type="MyObjectBuilder_CharacterHandItemsComponentDefinition">
