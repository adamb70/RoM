<?xml version="1.0" encoding="UTF-8"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  
  <Definition xsi:type="MyObjectBuilder_SeedBagHandItemDefinition">
    <Id Type="SeedBagHandItem" Subtype="SeedBagNorden_Barley"/>
		<Tag>SeedBag</Tag>
    <DisplayName>Barley Seed Bag</DisplayName>
    <Description>Barley, commonly found in the Highlands and Norden Plains.</Description>
    <Icon>Textures\GUI\Icons\ToolsAndWeapons\SeedBag.dds</Icon>
    <Icon>Textures\GUI\Icons\Universal\Farming\Norden_Barley.dds</Icon>
    <Size>
      <X>1.2</X>
      <Y>0.6</Y>
      <Z>0.3</Z>
    </Size>
    <Mass>0.5</Mass>
    <Model>Models\Weapons\SeedBag_Herbs.mwm</Model>
    <PhysicalMaterial>SeedBag</PhysicalMaterial>
		<Animation>SeedBag</Animation>
    <MaxStackAmount>1</MaxStackAmount>
<!-- Equipment -->
    <BearerDefinition Type="MyObjectBuilder_EntityEquipmentComponent" Subtype="Humanoid"/>
    <MaxDurability>25</MaxDurability>
    <Position>
      <Slot>MainHand</Slot>
    </Position>
    <Position>
      <Slot>OffHand</Slot>
    </Position>
    <!-- extra transform on IK dummy in 1st person -->
    <EquippedTransform>
      <!-- tool rotation offset from hand -->
      <Position>
        <X>-0.074</X>
        <Y>0.007</Y>
        <Z>0.215</Z>
      </Position>
      <Orientation>
        <X>-0.6394914</X>
        <Y>-0.5225991</Y>
        <Z>0.3265881</Z>
        <W>0.4596533</W>
      </Orientation>
    </EquippedTransform>
    <EquippedTransformFps>
      <!-- hand IK target offset -->
      <Position>
        <X>0.360</X>
        <Y>-0.258</Y>
        <Z>-0.339</Z>
      </Position>
      <Orientation>
        <X>0.3001874</X>
        <Y>0.6298291</Y>
        <Z>-0.2882942</Z>
        <W>0.6558119</W>
      </Orientation>
    </EquippedTransformFps>
<!-- Behavior -->
    <StanceToBehavior>
      <StanceName>NormalMode</StanceName>
      <BehaviorId Type="MyObjectBuilder_SeedingToolBehaviorDefinition" Subtype="SeedingNormal"/>
    </StanceToBehavior>
<!-- Seed bag definition -->
    <GrowableDefinitionId>
      <TypeId>MyObjectBuilder_FarmableEnvironmentItemDefinition</TypeId>
      <SubtypeId>FarmNorden_Barley</SubtypeId>
    </GrowableDefinitionId>
  </Definition>
  
