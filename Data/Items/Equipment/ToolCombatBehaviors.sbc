  <Definition xsi:type="MyObjectBuilder_MeleeWeaponBehaviorDefinition">
    <Id Type="MeleeWeaponBehaviorDefinition" Subtype="GenericToolCombatBehavior"/>

    <LeftSwing>
      <Range>1.8</Range>
