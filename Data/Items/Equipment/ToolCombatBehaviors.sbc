	  <StatCost Stat="Stamina" Cost="7" />
	  <StatCost Stat="Fury" Cost="-1" />
    </RightSwing>

    <ForwardSwing>
      <Range>1.8</Range>
      <Damage>8</Damage>
      <Arc>30</Arc>

      <HitTime>.6</HitTime>
      <AnimationTime>1</AnimationTime>
      <Animation>Attack</Animation>
	  
	  <SwingSoundDelay>0.3</SwingSoundDelay>
      <SwingSound>ToolSwish</SwingSound>
	  <StatCost Stat="Stamina" Cost="7" />
	  <StatCost Stat="Fury" Cost="-1" />
