    <Type>Player</Type>
  </Definition>
 
  <Definition xsi:type="MyObjectBuilder_UseObjectsComponentDefinition">
    <Id Type="MyObjectBuilder_UseObjectsComponent" Subtype="NomadTentAdv" />
