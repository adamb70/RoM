    <DisplayName>Advanced Linen Tent</DisplayName>
    <Icon>Textures\GUI\Icons\Cubes\NomadTentAdv.dds</Icon>
    <CubeSize>Small</CubeSize>
    <BlockTopology>TriangleMesh</BlockTopology>
    <Size x="7" y="6" z="7" />
