    <DisplayName>Tent</DisplayName>
    <Icon>Textures\GUI\Icons\Cubes\NomadTentAdv2.dds</Icon>
    <Category>SmallBlocks</Category>
    <Prerequisites>
      <Item Amount="1" Type="Block" Subtype="NomadBed" />
