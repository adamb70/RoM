      <Category>Cooking_Blue_Berry_Juice_Kegstand</Category>
    </CraftingCategories>
    <ProductionQueueSize>4</ProductionQueueSize>
    <CraftingSpeedMultiplier>1</CraftingSpeedMultiplier>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_UseObjectsComponentDefinition">
    <Id Type="MyObjectBuilder_UseObjectsComponent" Subtype="Blueberry_Kegstand" />
    <UseObjects>
      <UseObject Dummy="detector_inventory" Name="inventory"/>
    </UseObjects>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_InventoryComponentDefinition">
    <Id Type="MyObjectBuilder_Inventory" Subtype="Blueberry_KegstandInventory" />
    <InventoryName>Blueberry_KegstandInventory</InventoryName>
    <DisplayName>Blueberry_Kegstand</DisplayName>
    <MaxItemCount>28</MaxItemCount>
    <MultiplierEnabled>false</MultiplierEnabled>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_ContainerDefinition">
    <Id Type="CubeBlock" Subtype="Blueberry_Kegstand" />
    <Component Type="UseObjectsComponent" Subtype="Blueberry_Kegstand" ForceCreate="true"/>
    <Component Type="Inventory" Subtype="Blueberry_KegstandInventory" ForceCreate="true"/>
    <Component Type="InventorySpawnComponent"/>
    <Component Type="CraftingComponentQueued" Subtype="Blueberry_Kegstand" ForceCreate="true"/>
  </Definition>


</Definitions>