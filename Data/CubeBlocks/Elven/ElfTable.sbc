    <Model>Models\Cubes\Small\ElfTable.mwm</Model>
    <PhysicalMaterial>WoodHeavy</PhysicalMaterial>
    <Components>
      <Component Type="Block" Subtype="ElfTable" Count="1" />
    </Components>
    <MountPoints>
      <MountPoint Side="Bottom" StartX="1.4" StartY="1.5" EndX="8.6" EndY="5.5" />
      <MountPoint Side="Top" StartX="0" StartY="0" EndX="10" EndY="7" />
