  <Definition xsi:type="MyObjectBuilder_BuildableBlockDefinition">
    <Id Type="Block" Subtype="ElfTable" />
    <DisplayName>Elven Table</DisplayName>
    <Description>Elven Table</Description>
    <Icon>Textures\GUI\Icons\Cubes\ElfTable.dds</Icon>
