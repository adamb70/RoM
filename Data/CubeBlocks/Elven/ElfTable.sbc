  <Definition xsi:type="MyObjectBuilder_SchematicItemDefinition">
    <Id Type="SchematicItem" Subtype="Carpenter_Schematic_ElfTable" />
    <Public>true</Public>
    <DisplayName>Elven Table Manuscript</DisplayName>
    <Description>
      As you examine the manuscript, you notice 
      diagrams and written instructions explaining
      the process for crafting an Elven Table. This 
      knowledge would be valuable for a Carpenter.
    </Description>
    <Icon>Textures\GUI\Icons\ScrollBackground_1.dds</Icon>
    <Icon>Textures\GUI\Icons\Cubes\ElfTable.dds</Icon>
    <Icon>Textures\GUI\Icons\ScrollBackground_2.dds</Icon>
    <Size>
      <X>0.1</X>
      <Y>0.1</Y>
      <Z>0.1</Z>
    </Size>
    <Mass>0.25</Mass>
    <Model>Models\Cubes\small\Scroll.mwm</Model>
    <PhysicalMaterial>None</PhysicalMaterial>
    <MaxStackAmount>10</MaxStackAmount>
    <UseSound>PlayEat</UseSound>
    <Research Type="ResearchDefinition" Subtype="ElfTable_Carpenter" />
    <CanSpawnFromScreen>true</CanSpawnFromScreen>
