<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <Definition xsi:type="MyObjectBuilder_BuildableBlockDefinition">
    <Id Type="Block" Subtype="Reyjk_GeneralStore_Market_Stand" />
    <DisplayName>Reyjk General Store Market Stand</DisplayName>
    <Icon>Textures\GUI\Icons\Universal\Economy\MarketStand.dds</Icon>
    <Icon>Textures\GUI\Icons\NPC.dds</Icon>
    <GridDataDefinitionId Type="GridDataComponent" Subtype="SmallGridDataBase" />
    <BoundingBox>
      <Min x="0" y="-9" z="0" />
      <Max x="6" y="-2" z="4" />
    </BoundingBox>
    <Model>Models\Universal\Economy\market_stand.mwm</Model>
    <PhysicalMaterial>WoodHeavy</PhysicalMaterial>
    <Components>
      <Component Tag="Timber" Count="10" />
    </Components>
    <MountPoints>
      <MountPoint Side="Bottom" StartX="0.0" StartY="0.0" EndX="1.0" EndY="1.0" />
    </MountPoints>
    <Rotation>YAxis</Rotation>
    <MaxIntegrity>1200</MaxIntegrity>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_CraftingComponentDefinition">
    <Id Type="CraftingComponent" Subtype="ReyjkGeneralStoreMarketStandCrafting" />
    <InputInventory>InputInventory</InputInventory>
    <OutputInventory>OutputInventory</OutputInventory>
    <CraftingInventory>CraftingInventory</CraftingInventory>
    <ProductInventory>ProductInventory</ProductInventory>
    <PowerRequired>None</PowerRequired>
    <CraftingSpeedMultiplier>1</CraftingSpeedMultiplier>
    <ReturnPrerequisitesOnCancel>true</ReturnPrerequisitesOnCancel>
    <CraftString>Purchase</CraftString>
    <CraftingString>Purchasing Queue</CraftingString>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_CraftingSchedulerDefinition">
    <Id Type="CraftingScheduler" Subtype="ReyjkGeneralStoreMarketStandScheduler" />
    <CraftingComponent Type="CraftingComponent" Subtype="ReyjkGeneralStoreMarketStandCrafting" />
    <QueueSize>4</QueueSize>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_ConstantRecipeProviderComponentDefinition">
    <Id Type="ConstantRecipeProviderComponent" Subtype="ReyjkGeneralStoreMarketStandRecipes" />
    <Category>Reyjk_GeneralStore_Market_Stand</Category>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_ContainerDefinition" Copy="Append">
    <Id Type="Block" Subtype="Reyjk_GeneralStore_Market_Stand" />
    <CopyFrom Type="EntityBase" Subtype="BaseFatBlock" />

    <Component Type="CraftingComponent" Subtype="ReyjkGeneralStoreMarketStandCrafting" />
    <Component Type="CraftingScheduler" Subtype="ReyjkGeneralStoreMarketStandScheduler" />
    <Component Type="ConstantRecipeProviderComponent" Subtype="ReyjkGeneralStoreMarketStandRecipes" />

    <Component Type="Inventory" Subtype="InputInventory" />
    <Component Type="Inventory" Subtype="CraftingInventory" />
    <Component Type="Inventory" Subtype="ProductInventory" />
    <Component Type="Inventory" Subtype="OutputInventory" />

    <Component Type="InventorySpawnComponent" />
  </Definition>

</Definitions>